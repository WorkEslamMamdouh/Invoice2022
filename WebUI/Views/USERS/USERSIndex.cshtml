@using Inv.WebUI.Resources
@{

    Layout = "~/Views/Shared/Layout.cshtml";
}


<body class="materialdesign">
    <!--[if lt IE 8]>
            <p class="browserupgrade">You are using an <strong>outdated</strong> browser. Please <a href="http://browsehappy.com/">upgrade your browser</a> to improve your experience.</p>
        <![endif]-->
    <!-- Header top area start-->
    <div class="wrapper-pro">


        <!-- Breadcome start-->
        <div class="breadcome-area mg-b-30 small-dn">
            <div class="container-fluid">
                <div class="row">
                    <div class="col-lg-12">

                    </div>
                </div>
            </div>
        </div>

        <div class="admin-dashone-data-table-area mg-b-15">
            <div class="container-fluid">
                <div class="row">
                    <div class="col-lg-12">
                        <div class="sparkline8-list shadow-reset">
                            <div class="sparkline8-hd">
                                <div class="main-sparkline8-hd">
                                    <h1 style="text-align: center;">User</h1>
                                </div>
                                <br>
                                <div class="row">
                                    <div class="col-lg-3">
                                        <div class="input-mask-title">
                                            <label>Name</label>
                                        </div>
                                    </div>
                                    <div class="col-lg-3">
                                        <div class="input-mark-inner mg-b-22">
                                            <input type="text" class="form-control" placeholder="name">
                                        </div>
                                    </div>
                                    <div class="col-lg-3">
                                        <div class="input-mask-title">
                                            <label>phone</label>
                                        </div>
                                    </div>
                                    <div class="col-lg-3">
                                        <div class="input-mark-inner mg-b-22">
                                            <input type="tel" class="form-control" placeholder="">
                                        </div>
                                    </div>
                                </div>
                                <div class="row">
                                    <div class="col-lg-3">
                                        <div class="input-mask-title">
                                            <label>user name</label>
                                        </div>
                                    </div>
                                    <div class="col-lg-3">
                                        <div class="input-mark-inner mg-b-22">
                                            <input type="text" class="form-control" placeholder="user name">
                                        </div>
                                    </div>
                                    <div class="col-lg-3">
                                        <div class="input-mask-title">
                                            <label>password</label>
                                        </div>
                                    </div>
                                    <div class="col-lg-3">
                                        <div class="input-mark-inner mg-b-22">
                                            <input type="password" class="form-control" placeholder="password">
                                        </div>
                                    </div>
                                </div>
                            </div>
                            <div class="sparkline8-graph">
                                <div class="datatable-dashv1-list custom-datatable-overright">
                                    <table id="table" data-toggle="table" data-pagination="false" data-resizable="true" data-cookie="true" data-cookie-id-table="saveId" data-show-export="false" data-click-to-select="true" data-toolbar="#toolbar">
                                        <thead>
                                            <tr>
                                                <th class="bs-checkbox " style="width: 2.9%;" data-field="state" tabindex="0"><div class="th-inner "><input name="btSelectAll" type="checkbox"></div><div class="fht-cell"></div></th>
                                                <th data-field="" data-editable="false">User Permissions</th>
                                            </tr>
                                        </thead>
                                        <tbody>
                                            <tr>
                                                <td class="bs-checkbox ">
                                                    <input data-index="0" name="btSelectItem" type="checkbox">
                                                </td>
                                                <td>
                                                    <div class="interested-input-area state-success">
                                                        <select name="interestedcategory" class="valid">
                                                            <option value="none" selected="" disabled="">Interested in</option>
                                                            <option value="design">Web Design</option>
                                                            <option value="development">Web Development</option>
                                                            <option value="illustration">Wordpress Pro</option>
                                                            <option value="branding">Joomla Pro</option>
                                                            <option value="video">Video Marketing</option>
                                                        </select>
                                                    </div>
                                                </td>
                                            </tr>
                                        </tbody>
                                    </table>
                                </div>
                                <br>
                                <div class="btn-group project-list-action">
                                    <button class="btn btn-custon-four btn-success"><i class="fa fa-create"></i>create</button>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</body>
